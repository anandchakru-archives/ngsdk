{
  "name": "@nivite/ngsdk",
  "version": "1.0.33",
  "description": "Angular Library to create online invitations the nerdy way.",
  "keywords": [
    "invite",
    "rsvp",
    "wedding",
    "birthday",
    "graduation",
    "festival",
    "diy",
    "party",
    "gettogether",
    "potluck",
    "celebration",
    "picnic",
    "invitation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nivite/ngsdk"
  },
  "homepage": "https://nivite.jrvite.com",
  "author": "Anand Rockzz",
  "bugs": {
    "url": "https://github.com/nivite/ngsdk/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run start:lib && npm run start:app",
    "start:app": "npm run v && ng serve ngsdk-app --port 4500",
    "start:lib": "npm run v && ng build ngsdk-lib --watch",
    "build": "npm run build:lib && npm run build:lib",
    "build:app": "ng build",
    "build:lib": "npm run v && ng build ngsdk-lib",
    "build:stats": "ng build --stats-json",
    "build:anal": "webpack-bundle-analyzer dist/ngsdk-app/stats-es5.json",
    "pack:lib": "npm run build:lib && cd dist/ngsdk-lib && npm pack",
    "pub:lib": "npm run pack:lib && npm publish dist/ngsdk-lib/nivite-ngsdk-${npm_package_version}.tgz",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "v": "node config/version.js"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@types/file-saver": "^2.0.1",
    "@types/gapi": "0.0.39",
    "@types/gapi.client": "^1.0.1",
    "@types/jquery": "^3.3.30",
    "@types/moment-timezone": "^0.5.12",
    "angularfire2": "^5.2.1",
    "bootstrap": "^4.3.1",
    "file-saver": "^2.0.2",
    "firebase": "^6.3.4",
    "jquery": "^3.4.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.26",
    "ngx-markdown": "^8.1.0",
    "popper.js": "^1.15.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular-devkit/build-ng-packagr": "~0.800.6",
    "@angular/cli": "~8.0.1",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-bundle-analyzer": "^3.4.1"
  }
}
